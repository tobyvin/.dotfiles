[calendars]
[[personal]]
path = $XDG_DATA_HOME/calendars/personal

[[family]]
path = $XDG_DATA_HOME/calendars/family

[[shows]]
path = $XDG_DATA_HOME/calendars/shows

[[movies]]
path = $XDG_DATA_HOME/calendars/movies

[[holidays]]
path = $XDG_DATA_HOME/calendars/holidays

[[birthdays]]
path = $XDG_DATA_HOME/contacts/personal
type = birthdays

[keybindings]
save = w
external_edit = e
export = x

[default]
default_calendar = personal

[locale]
default_timezone = Us/Central
timeformat = %H:%M
dateformat = %y/%m/%d
longdateformat = %Y/%m/%d
datetimeformat = %y/%m/%d %H:%M
longdatetimeformat = %Y/%m/%d %H:%M

[palette]
edit = 'white', '', '', 'white', ''
edit focus = 'white', 'black', '', 'white', 'black'
